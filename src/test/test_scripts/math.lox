// Add/Sub
var a = 1;
var b = 2;
var c = a + b;
assert(c == 3);
assert(-c == -3);
assert(c - a - b == 0);


//Mult/Divide
var d = c * b;
assert(d == 6);
assert(d / b == c);
assert(c / b == 1.5);
assert((a + a) / (4 - 2) == 1);

//Compare
assert(c <= c);
assert(c <= d);
assert(c < d);

assert(c >= c);
assert(d >= c);
assert(d > c);

assert(c != d);

assert(1);
assertFalse(0);